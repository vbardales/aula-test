{
  "name": "aula-test",
  "version": "1.0.0",
  "description": "REST API about songs",
  "main": "index.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch server server/index.js --exec babel-node",
    "test": "cross-env NODE_ENV=test mocha --require babel-register ./mocha.conf.js server --recursive --exclude server/index.js",
    "lint": "eslint server",
    "prebuild": "rimraf dist",
    "postbuild": "cp -Rp package*.json dist",
    "build": "babel server -d dist"
  },
  "author": "Virginie BARDALES",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "~6.26.0",
    "bluebird": "~3.5.5",
    "body-parser": "~1.19.0",
    "compression": "~1.7.4",
    "express": "~4.17.1",
    "helmet": "~3.18.0",
    "joi": "~14.3.1",
    "lodash": "~4.17.11",
    "method-override": "~3.0.0",
    "multer": "^1.4.1",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~10.0.1",
    "babel-plugin-istanbul": "~5.1.0",
    "babel-plugin-transform-builtin-extend": "~1.1.2",
    "babel-preset-env": "~1.7.0",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.26.0",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "cross-env": "~5.2.0",
    "dirty-chai": "~2.0.1",
    "eslint": "~5.16.0",
    "mocha": "~6.1.4",
    "nodemon": "~1.19.1",
    "rimraf": "~2.6.3",
    "sinon": "~7.3.2",
    "sinon-chai": "~3.3.0"
  }
